{
  "name": "time",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "7.10.1"
  },
  "scripts": {
    "clean": "rimraf ./dist; mkdir -p dist/public",
    "dev": "concurrently \"npm run dev:app\" \"npm run dev:server\"",
    "dev:app": "npm run prebuild:app && ng build --watch && ng build --watch --app time-client-universal",
    "dev:server": "npm run build:server && concurrently \"npm run watch:server\" \"nodemon dist/server.js\"",
    "prebuild:app": "node ./build-utils/time-client.prebuild",
    "prebuild:api": "node ./build-utils/time-api.prebuild",
    "build": "npm run clean; npm run build:app && npm run build:server",
    "build:staging": "npm run clean; npm i; npm run build:staging-app && npm run build:server",
    "build:prod": "npm run clean; npm i; npm run build:prod-app && npm run build:server",
    "build:app": "npm run prebuild:app && ng build && ng build --app time-client-universal",
    "build:staging-app": "npm i; npm run prebuild:app && ng build --prod --environment=staging && ng build --app time-client-universal --prod --environment=staging",
    "build:prod-app": "npm run prebuild:app && ng build --prod --environment=prod && ng build --prod --environment=prod --app time-client-universal",
    "build:server": "npm run prebuild:api && webpack --config time-api/config/webpack.server-config.js",
    "test:app": "ng test",
    "lint:app": "ng lint",
    "e2e:app": "ng e2e",
    "watch:server": "webpack --config time-api/config/webpack.server-config.js --watch",
    "start": "node dist/server",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./time-client/protractor.conf.js"
  },
