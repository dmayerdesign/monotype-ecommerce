{
  "name": "time",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "7.10.1"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:app\" \"npm run dev:server\"",
    "dev:app": "concurrently \"ng build --watch --app time-client-universal\" \"ng build --watch\" ",
    "dev:server": "npm run build:server && concurrently \"npm run watch:server\" \"cd time-client; nodemon dist/server.js\"",
    "build:prod": "rimraf ./time-client/dist; mkdir -p time-client/dist/public; npm run build:prod-app && npm run build:server",
    "build:staging": "rimraf ./time-client/dist; mkdir -p time-client/dist/public; npm run build:staging-app && npm run build:server",
    "build:app": "ng build; ng build --app time-client-universal",
    "build:prod-app": "npm i; ng build --prod --no-aot --environment=prod && ng build --app time-client-universal --prod --no-aot --environment=prod",
    "build:staging-app": "npm i; ng build --prod --no-aot --environment=staging && ng build --app time-client-universal --prod --no-aot --environment=staging",
    "build:server": "webpack --config time-client/config/webpack.server-config.js",
    "test:app": "ng test",
    "lint:app": "ng lint",
    "e2e:app": "ng e2e",
    "watch:server": "webpack --config time-client/config/webpack.server-config.js --watch",
    "start": "node time-client/dist/server",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./time-client/protractor.conf.js"
  },
  "dependencies": {
    "@nguniversal/express-engine": "^1.0.0-beta.2",
