{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@angular/*": ["./node_modules/@angular/*"],
      "@time/common/*": [ "./time-common/*" ],
      "@time/app-config": [ "./app-config.ts" ],
      "stripe": ["./node_modules/@types/stripe-node"]
    },
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "types": [ "node" ],
    "typeRoots": [ "./node_modules/@types" ],
    "noImplicitAny": false,
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "allowJs": true
  },
  "outDir": "tsc/",
  "compileOnSave": false,
  "exclude": [
    "node_modules",
    "dist/"
  ]
}
