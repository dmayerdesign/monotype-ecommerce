{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "es6",
      "es2016",
      "dom"
    ],
    "noImplicitAny": false,
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noUnusedLocals": false
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules",
    "source/client"
  ]
}
