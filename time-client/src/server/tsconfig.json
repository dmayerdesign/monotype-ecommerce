{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "es6",
      "es2016",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@time/*": [
        "../../../time-common/*"
      ],
      "@time/models": [
        "../../../time-common/models/db-models"
      ],
      "@time/interfaces": [
        "../../../time-common/models/interfaces"
      ]
    },
    "noImplicitAny": false,
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noUnusedLocals": false
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules"
  ]
}
