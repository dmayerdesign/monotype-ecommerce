{
  "name": "time-client",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "7.10.1"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:app\" \"npm run dev:server\"",
    "dev:app": "ng build --watch",
    "dev:server": "npm run build:server && concurrently \"npm run watch:server\" \"nodemon dist/server.js\"",
    "build:prod": "rimraf ../time-client-deploy/dist; mkdir -p ../time-client-deploy/dist; npm run build:prod-app && npm run build:prod-server",
    "build:app": "ng build",
    "build:prod-app": "cd ../time-client-deploy; npm i; cd ../time-client; ng build --prod --environment=prod && cp -r dist/public ../time-client-deploy/dist",
    "build:server": "webpack --config config/webpack.server-config.js",
    "build:prod-server": "cp .env ../time-client-deploy/.env;  webpack --config config/webpack.prod-server-config.js",
    "test:app": "ng test",
    "lint:app": "ng lint",
    "e2e:app": "ng e2e",
    "watch:server": "webpack --config config/webpack.server-config.js --watch",
    "start": "node dist/server",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js",
    "heroku:deploy": "../deploy.sh",
    "heroku:release": "../release.sh"
  },
  "dependencies": {
    "body-parser": "1.17.2",
    "connect-mongo": "1.3.2",
    "core-js": "2.4.1",
    "dotenv": "4.0.0",
