    "fs-extra": "4.0.0",
    "helmet": "3.6.1",
    "inversify": "4.2.0",
    "inversify-binding-decorators": "3.0.0",
    "inversify-express-utils": "4.0.0",
    "inversify-logger-middleware": "3.0.0",
    "jasmine": "2.6.0",
    "jasmine-node": "1.14.5",
    "mongodb": "2.2.30",
    "mongoose": "4.11.4",
    "morgan": "1.8.2",
