{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "mte"
  },
  "apps": [
    {
      "name": "mte-client",
      "root": "mte-client/src",
      "outDir": "./dist/public",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "../tsconfig.app.json",
      "testTsconfig": "../tsconfig.spec.json",
      "prefix": "mte",
      "styles": [
        "styles.scss"
      ],
      "stylePreprocessorOptions": {
        "includePaths": [
          "styles"
        ]
      },
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "staging": "environments/environment.staging.ts",
        "prod": "environments/environment.prod.ts"
      }
    },
    {
      "name": "mte-client-universal",
      "platform": "server",
      "root": "mte-client/src",
      "outDir": "./dist/server-app",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.server.ts",
      "test": "test.ts",
      "tsconfig": "../tsconfig.server.json",
      "testTsconfig": "../tsconfig.spec.json",
      "prefix": "mte",
      "styles": [
        "styles.scss"
      ],
      "stylePreprocessorOptions": {
        "includePaths": [
          "styles"
        ]
      },
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "staging": "environments/environment.staging.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./mte-client/protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "mte-client/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "mte-client/src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "mte-client/e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./mte-client/karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {
    }
  }
}
  